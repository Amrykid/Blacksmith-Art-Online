    //keyboard movement
    if keyboard_check(vk_right){
        x += 4
        sprite_assign(spr_still, spr_right);
        right = true
        left = false
        up = false
        down = false
    }
    if keyboard_check(vk_left){
        x -= 4
        sprite_assign(spr_still, spr_left);
        left = true
        right = false
        up = false
        down = false
    }
    if keyboard_check(vk_up){
        y -= 4
        sprite_assign(spr_still, spr_up);      
        up = true
        left = false
        right = false
        down = false
    }
    if keyboard_check(vk_down){
        y +=4
        image_speed = room_speed/2
        sprite_assign(spr_still, spr_down);
        down = true
        left = false
        up = false
        right = false
    }
    //slash
    if keyboard_check(ord("Z")){
        if can_swing = true{
            if up = true{
                instance_create(x, y-16, Ob_Slash)
            }
            if down = true{
                instance_create(x, y+16, Ob_Slash)
            }
                if right = true{
            instance_create(x + 16, y, Ob_Slash)
            }
                if left = true{
            instance_create(x - 16, y, Ob_Slash)
            }
        can_swing = false
        can_move = false
        alarm[0] = 10
        }
    }
    //smash
    if keyboard_check(ord("X")){
        if can_swing = true{
            if up = true{
                instance_create(x, y-16, Ob_Smash)
            }
            if down = true{
                instance_create(x, y+16, Ob_Smash)
            }
            if right = true{
                instance_create(x + 16, y, Ob_Smash)
            }
            if left = true{
                instance_create(x - 16, y, Ob_Smash)
            }
            can_swing = false
            can_move = false
            alarm[0] = 10
        }
    }
    if x == xprevious &amp;&amp; y == yprevious{
    image_speed = 0;
    image_index = 0;
    }
    else{
    image_speed = 0.4}
}
</string>
